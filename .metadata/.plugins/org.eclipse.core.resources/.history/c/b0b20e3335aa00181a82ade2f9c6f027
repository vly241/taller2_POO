package taller2_2;
import java.util.Scanner;
class taller2_2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc = new Scanner(System.in);
		
		int numero;
		int n2;
		int n3;
		int n4;
		
		System.out.println("ingrese el numero: ");
		numero = sc.nextInt();
		
		
	
	sc.close();
		n2 = numero*2;
		n3 = numero*3;
		n4 = numero*4;
		
		System.out.println("el nuemro: "+ numero +" y su doble es: "+ n2 +" su triple es: "+ n3 +" y su cuadruple es: "+ n4);
	}

}
